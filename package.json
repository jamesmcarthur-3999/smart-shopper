{
  "name": "smart-shopper",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered shopping assistant with fast, interactive product recommendations",
  "scripts": {
    "prestart": "node scripts/kill-port.js",
    "start": "node scripts/generate-env.js && react-scripts start",
    "preserver": "node scripts/kill-port.js",
    "server": "node server.js",
    "dev": "node scripts/kill-port.js && concurrently \"npm run start\" \"npm run server\"",
    "build": "node scripts/generate-env.js && react-scripts build",
    "test": "react-scripts test",
    "test:performance": "jest --config jest.performance.config.js",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,css}'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "kill-ports": "node scripts/kill-port.js"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.13",
    "@types/react": "^17.0.48",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "concurrently": "^8.2.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-storybook": "^0.6.4",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "nodemon": "^3.0.1",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8"
  },
  "proxy": "http://localhost:3001",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
