NODE_ENV=development
SKIP_PREFLIGHT_CHECK=true
TSC_COMPILE_ON_ERROR=true
BROWSER=none
PORT=3000
SERVER_PORT=3001
REACT_APP_API_URL=http://localhost:3001/api

# API Endpoints
SERPAPI_ENDPOINT=https://serpapi.com/search
SEARCH1_ENDPOINT=https://api.search1.com/search
PERPLEXITY_ENDPOINT=https://api.perplexity.ai/search
CLAUDE_ENDPOINT=https://api.anthropic.com/v1/messages

# Feature Flags
ENABLE_CACHING=true
DEFAULT_GRID_LAYOUT=3x3
MAX_CARDS_PER_VIEW=12
ENABLE_ANIMATIONS=true

# Claude Model Configuration
CLAUDE_MODEL=claude-3-7-sonnet-20250219

# API Keys (DO NOT COMMIT ACTUAL KEYS)
# SERPAPI_API_KEY=your_key_here
# SEARCH1_API_KEY=your_key_here
# PERPLEXITY_API_KEY=your_key_here
# CLAUDE_API_KEY=your_key_here
